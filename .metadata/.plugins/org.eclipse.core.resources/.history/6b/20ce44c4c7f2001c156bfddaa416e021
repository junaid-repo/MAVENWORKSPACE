package java8;

import java.util.*;
import java.util.function.Function;

public class FP03MehodReferences {
	
	private static void printIt(String str) {
		System.out.print(str+" ");
	}
	
	private static String changeToUpper(String str) {
		return str.toUpperCase();
	}

	public static void main(String[] args) {
		List<String> str = List.of("Arrays", "String", "Boolean", "Integer", "Double");
		
		
		

		//Function<String,String> mapper = x -> x.toUpperCase();
		//extracted(str, x -> x.toUpperCase());
		//extracted(str, x -> x.toLowerCase());
		
		str.stream().map(FP03MehodReferences::changeToUpper).forEach(System.out::print);;



	}

	private static void extracted(List<String> str, Function<String, String> mapper) {
		str.stream().map(mapper).forEach(x -> FP03MehodReferences.printIt(x));
		System.out.println();
	}

}

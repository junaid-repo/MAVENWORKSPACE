package java8;

import java.util.List;
import java.util.function.Predicate;

public class F03FunctionalInterface {

	public static void main(String[] args) {
		List<Integer> numbers = List.of(45, 2, 8, 5, 10);

		Predicate<? super Integer> isEvenPredicate = i -> i % 2 == 0;
		numbers.stream().filter(isEvenPredicate).map(i -> i * i).forEach(System.out::println);
	}

}

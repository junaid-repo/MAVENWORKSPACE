package java8;

import java.util.List;
import java.util.function.BinaryOperator;
import java.util.function.Consumer;
import java.util.function.Function;
import java.util.function.Predicate;

public class F04BehaviourParamaterization {

	public static void main(String[] args) {
		List<Integer> numbers = List.of(45, 2, 8, 5, 10);

		Predicate<Integer> checkPredicate = i -> i % 2 == 1;
		extracted(numbers, checkPredicate);

	}

	private static void extracted(List<Integer> numbers, Predicate<Integer> checkPredicate) {
		numbers.stream().filter(checkPredicate).forEach(System.out::println);
	}
}
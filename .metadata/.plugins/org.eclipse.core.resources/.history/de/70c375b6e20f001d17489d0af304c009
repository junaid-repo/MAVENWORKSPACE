package java8Basics;

import java.util.List;

public class F01Functional {

	public static void main(String[] args) {
		printCubesOfEvenNumber(List.of(43, 52, 98, 12, 30, 64));
	}

	private static void printCubesOfEvenNumber(List<Integer> numbers) {

		numbers.stream().filter(i -> i % 2 == 0).map(j -> j * j).forEach(System.out::println);
	}

}

package java8;

import java.util.ArrayList;
import java.util.List;
import java.util.stream.Collectors;

public class FP01Structured {

	public static void main(String[] args) {
		printAllNumbersInListStructured(List.of(3, 4, 1, 56, 34));
		List<Integer> numbers = List.of(754, 45, 67, 234, 56, 72, 48);
		List<String> courses = List.of("Boot", "Spring FrameWork", "API", "Web Services", "Spring");

		printFnEven(numbers);

		// courses.stream().forEach(System.out::println);
		courses.stream().filter(i -> i.contains("Spring")).forEach(System.out::println);
		courses.stream().map(i -> i.length()).filter(i->i>4).map(i->i).forEach(System.out::println);

	}

	public static boolean printEven(int n) {

		return n % 2 == 0;
	}

	public static void printAllNumbersInListStructured(List<Integer> numbers) {

		/*
		 * for (Integer numb : numbers) { System.out.println(numb); }
		 */
	}

	public static void printFnEven(List<Integer> numbers) {

		/*
		 * for (Integer num : numbers) if (printEven(num) == true)
		 * System.out.println(num);
		 */

		// numbers.stream().filter(i -> i % 2 == 0).forEach(System.out::println);

	}

}

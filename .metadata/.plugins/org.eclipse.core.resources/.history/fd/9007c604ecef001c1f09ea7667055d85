package java8;

import java.util.ArrayList;
import java.util.List;

public class FP01Structured {

	public static void main(String[] args) {
		printAllNumbersInListStructured(List.of(3, 4, 1, 56, 34));
		List<Integer> numbers = List.of(234, 45, 67, 234, 56, 72, 48);
		printFnEven(numbers);

	}

	public static boolean printEven(int n) {

		return n % 2 == 0;
	}

	public static void printAllNumbersInListStructured(List<Integer> numbers) {

		for (Integer numb : numbers) {
			System.out.println(numb);
		}
	}

	public static void printFnEven(List<Integer> numbers) {

		for(Integer num: numbers)
			if(printEven(num)==true)
				System.out.println(num);;
	}

}

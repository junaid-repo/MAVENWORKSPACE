package com.example.demo;

import java.io.File;
import java.io.FileNotFoundException;
import java.io.FileReader;
import java.util.ArrayList;
import java.util.Scanner;

public class AbstractTest extends AbstractC{
	
	public static void main(String[] args) {
		
		AbstractTest ob =  new AbstractTest();
		/*
		 * ob.abstractTest1(); ob.abstractTest2(); ob.abstractTest3();
		 */
		int removeNum;
		
		ArrayList<Integer> list = new ArrayList<>();
		list.add(12);
		list.add(434);
		list.add(345);
		list.add(90);
		list.add(66);
		
		System.out.println("Enter a number to remove");
		Scanner sc = new Scanner(System.in);
		
		removeNum=sc.nextInt();
		int count=0;
		for(int i:list) {
			
			if( i==removeNum) {
				list.remove(count);}
			count++;
		}
		System.out.println(list);
		
	
		StringBuilder bb = new StringBuilder();
		bb.append(sc.nextLine());
		 File file = new File(
		            "C:\\Users\\pankaj\\Desktop\\test.txt");
		 
		 File fl = new File(
				 "C:\\Users\\junai\\OneDrive\\Documents\\virtualMachine_details.txt");
		 
			try {
				Scanner scr= new Scanner(fl);
				while(scr.hasNextLine())
					System.out.println(scr.nextLine());
			} catch (FileNotFoundException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
				
				
			}
		 
	}
	
	
	
	
	
	@Override
	public void abstractTest1() {
		System.out.println("Here is abstract1 called");
	}

	@Override
	public void abstractTest2() {
		System.out.println("Here is abstract2 called");
		
	}

	@Override
	public void abstractTest3() {
		System.out.println("Here is abstract3 called");
		
	}

